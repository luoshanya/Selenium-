[{"\u804c\u4f4d": "Python\u722c\u866b\u5de5\u7a0b\u5e08", "\u516c\u53f8": "\u97e9\u56fd\u74f7\u808c\u4e2d\u56fd\u533a\u516c\u53f8", "\u85aa\u8d44": "0.8-1.3\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u5458\u5de5\u65c5\u6e38\u4e13\u4e1a\u57f9\u8bad\u5b9a\u671f\u4f53\u68c0\u4e94\u9669\u4e00\u91d1", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde-\u5929\u6cb3\u533a|3-4\u5e74\u7ecf\u9a8c|\u5927\u4e13|\u62db2\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "\u5929\u6cb3\u533a\u73e0\u6c5f\u65b0\u57ce\u534e\u590f\u8def30\u53f7\u5bcc\u529b\u76c8\u901a\u5927\u53a617\u697c\u97e9\u56fd\u74f7\u808c\u4e2d\u56fd\u533a\u516c\u53f8\u6c11\u8425\u516c\u53f8500-1000\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u9ad8\u7ea7\u8f6f\u4ef6\u5de5\u7a0b\u5e08", "\u516c\u53f8\u5730\u5740": "\u5929\u6cb3\u533a\u73e0\u6c5f\u65b0\u57ce\u534e\u590f\u8def30\u53f7\u5bcc\u529b\u76c8\u901a\u5927\u53a617\u697c"}, {"\u804c\u4f4d": "\u722c\u866b\u5de5\u7a0b\u5e08", "\u516c\u53f8": "\u5e7f\u5dde\u6bd4\u5730\u6570\u636e\u79d1\u6280\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "0.7-1\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u4e94\u9669\u4e00\u91d1\u9910\u996e\u8865\u8d34\u4e13\u4e1a\u57f9\u8bad\u5e74\u7ec8\u5956\u91d1", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde-\u5929\u6cb3\u533a|2\u5e74\u7ecf\u9a8c|\u672c\u79d1|\u62db2\u4eba|03-14\u53d1\u5e03|\u8ba1\u7b97\u673a\u79d1\u5b66\u4e0e\u6280\u672f", "\u804c\u4f4d\u4fe1\u606f": "\u804c\u8d23\uff1a1.\u57fa\u4e8e\u516c\u53f8\u5185\u90e8\u91c7\u96c6\u6846\u67b6\uff0c\u8fdb\u884c\u6a21\u677f\u914d\u7f6e\uff0c\u5e76\u5229\u7528Java\u8fdb\u884c\u722c\u866b\u7a0b\u5e8f\u5f00\u53d1\uff1b2.\u7ef4\u62a4\u5185\u90e8\u7684\u722c\u866b\u6a21\u677f\uff1b3.\u9886\u5bfc\u5e03\u7f6e\u7684\u5176\u4ed6\u5de5\u4f5c\u5b89\u6392\u3002\u8981\u6c421.\u672c\u79d1\u53ca\u4ee5\u4e0a\u7684\u8ba1\u7b97\u673a\u53ca\u76f8\u5173\u4e13\u4e1a\u5b66\u5386\uff1b\u4e24\u5e74\u4ee5\u4e0a\u722c\u866b\u5de5\u4f5c\u7ecf\u9a8c\uff1b2.\u719f\u6089Http\u534f\u8bae\uff0c\u4e86\u89e3\u5e38\u89c1Header\u3001\u72b6\u6001\u7801\uff0c\u4e86\u89e3Ajax\u8bf7\u6c42\uff1b3.\u719f\u6089Html\u3001JavaScript\u3001JSON\u6570\u636e\u683c\u5f0f\uff0c\u638c\u63e1\u6b63\u5219\u8868\u8fbe\u5f0f\uff1b4.Java\u57fa\u7840+\u6570\u636e\u5e93\u57fa\u7840\u624e\u5b9e\uff1b5.\u8e0f\u5b9e\u80af\u5e72\uff0c\u6709\u8d23\u4efb\u5fc3\u548c\u56e2\u961f\u534f\u4f5c\u7cbe\u795e;6.\u719f\u6089\u719f\u6089\u516b\u722a\u9c7c\u91c7\u96c6\u5668\u3001Jsoup\u3001JQuery\u7684CSS\u9009\u62e9\u5668\u8005\u3001WebMagic\uff08Java\uff09\u3001Spring\u3001Hibernate/Mybatis\u8005\u3001Eclipse/MyEclipse\u7684\u4f7f\u7528\u6216\u6709\u722c\u866b\u76f8\u5173\u5de5\u4f5c\u7ecf\u9a8c\uff1b\u719f\u6089\u5176\u4e2d\u4e00\u5230\u4e24\u9879\u80fd\u529b\u8005\u4f18\u5148\u8003\u8651\u3002\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u4e94\u5c71\u8def\u79d1\u534e\u8857251\u53f7\u4e50\u5929\u521b\u610f\u56edB\u533a4007\u5e7f\u5dde\u6bd4\u5730\u6570\u636e\u79d1\u6280\u6709\u9650\u516c\u53f8\u6c11\u8425\u516c\u53f850-150\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u6570\u636e\u5e93\u5de5\u7a0b\u5e08/\u7ba1\u7406\u5458\u4e94\u9669\u4e00\u91d1", "\u516c\u53f8\u5730\u5740": "\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u4e94\u5c71\u8def\u79d1\u534e\u8857251\u53f7\u4e50\u5929\u521b\u610f\u56edB\u533a4007"}, {"\u804c\u4f4d": "\u9ad8\u7ea7\u722c\u866b\u5f00\u53d1\u5de5\u7a0b\u5e08", "\u516c\u53f8": "\u5e7f\u5dde\u94f6\u8baf\u4fe1\u606f\u79d1\u6280\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "1-1.5\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u5468\u672b\u53cc\u4f11\u8282\u65e5\u798f\u5229\u9910\u996e\u8865\u8d34\u52a0\u73ed\u8865\u8d34\u5e26\u85aa\u5e74\u5047\u7ee9\u6548\u5956\u91d1\u505a\u4e94\u4f11\u4e8c", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde-\u5929\u6cb3\u533a|3-4\u5e74\u7ecf\u9a8c|\u62db\u82e5\u5e72\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "\u5c97\u4f4d\u804c\u8d23\uff1a1\u3001\u5206\u5e03\u5f0f\u7f51\u7edc\u722c\u866b\u7cfb\u7edf\u53ca\u6570\u636e\u5e93\u7684\u8bbe\u8ba1\u4e0e\u5f00\u53d1\uff1b2\u3001\u8bbe\u8ba1\u722c\u866b\u7b56\u7565\u548c\u9632\u5c4f\u853d\u89c4\u5219\uff0c\u89e3\u51b3\u5c01\u8d26\u53f7\u3001\u5c01IP\u3001\u9a8c\u8bc1\u7801\u7b49\u96be\u70b9\u653b\u514b\uff1b3\u3001\u7f16\u5199\u5927\u89c4\u6a21\u5206\u5e03\u5f0f\u7f51\u7edc\u722c\u866b\uff08\u591a\u7ebf\u7a0b\u3001\u5206\u5e03\u5f0f\u8c03\u5ea6\u3001\u534f\u8bae\u5904\u7406\uff09\uff1b5\u3001\u6570\u636e\u6e05\u6d17\u6574\u7406\u7c7b\u4ea7\u54c1\u7684\u5e94\u7528\u5f00\u53d1\u3001\u4f18\u5316\uff1b\u6784\u5efa\u548c\u5f00\u53d1\u5728\u6d77\u91cf\u6570\u636e\u4e0b\u591a\u7ef4\u5206\u6790\u7684\u6570\u636e\u5206\u6790\u5e73\u53f06\u3001\u7cbe\u901a\u7f51\u9875\u6293\u53d6\u539f\u7406\u53ca\u6280\u672f\u3001\u6df1\u5ea6\u6293\u53d6\u3001\u52a8\u6001\u7f51\u9875\u6280\u672f\u6293\u53d6\u3001\u6d4f\u89c8\u5668\u6a21\u62df\u6293\u53d6\u6280\u672f\uff0c\u4ece\u7ed3\u6784\u5316\u7684\u548c\u975e\u7ed3\u6784\u5316\u7684\u6570\u636e\u4e2d\u83b7\u53d6\u4fe1\u606f\uff1b7\u3001\u7cbe\u901a\u722c\u866b\u548c\u53cd\u722c\u6280\u672f\uff0c\u7cbe\u901ahttp\u5e95\u5c42\u534f\u8bae\uff1b\u7cbe\u901a\u6df1\u5ea6\u6293\u53d6\u3001\u52a8\u6001\u7f51\u9875\u6280\u672f\u6293\u53d6\u3001\u6d4f\u89c8\u5668\u6a21\u62df\u6293\u53d6\u6280\u672f\u7b49\uff1b8\u3001\u638c\u63e1Java\u8bed\u8a00\uff0c\u5177\u6709\u5f00\u53d1JavaWeb\u7684\u80fd\u529b\uff0c\u5bf9SpringBoot\u3001SpringMVC\u7b49\u6846\u67b6\u6709\u89c1\u89e3\u30029\u3001\u719f\u6089HtmlParser/HttpClient/Jsoup/jQuery/bootstrap\u4e2d\u7684\u4e00\u79cd\u6216\u591a\u79cd\u5f00\u6e90\u6280\u672f\u30021\u3001\u8d1f\u8d23\u56fe\u50cf\u5904\u7406\u548c\u56fe\u50cf\u8bc6\u522b\u7684\u6280\u672f\u7814\u53d1\u30022\u3001\u8d1f\u8d23\u516c\u53f8\u56fe\u50cf\u8bc6\u522b\u5e73\u53f0\u7684\u4ea7\u54c1\u7814\u7a76\uff0c\u6280\u672f\u7814\u7a76\u30023\u3001\u8d1f\u8d23\u8ba1\u7b97\u673a\u89c6\u89c9\u5e94\u7528\u5f00\u53d1\u3001\u79fb\u690d\u53ca\u4f18\u53164\u3001\u8d1f\u8d23\u56fe\u50cf\u5904\u7406\u3001\u89c6\u9891\u5206\u6790\u53ca\u76ee\u6807\u8bc6\u522b\u7b97\u6cd5\u7684\u7814\u53d1\u53ca\u6027\u80fd\u4f18\u5316\u5de5\u4f5c5\u3001\u8d1f\u8d23\u76f8\u5e94\u65b9\u5411\u6280\u672f\u96be\u70b9\u653b\u5173\u3001\u524d\u77bb\u7814\u7a76\u4efb\u804c\u8981\u6c42\uff1a1.\u6a21\u5f0f\u8bc6\u522b\u3001\u56fe\u50cf\u5904\u7406\u3001\u5e94\u7528\u6570\u5b66\u7b49\u76f8\u5173\u4e13\u4e1a\u672c\u79d1\u53ca\u4ee5\u4e0a\u5b66\u5386\u30022.\u6709\u4e00\u5b9a\u7684\u673a\u5668\u89c6\u89c9\u6216\u56fe\u50cf\u5904\u7406\u65b9\u9762\u5de5\u4f5c\u7ecf\u9a8c\uff1b3.\u6709\u9a8c\u8bc1\u7801\u8bc6\u522b/\u673a\u5668\u5b66\u4e60\uff0f\u6df1\u5ea6\u5b66\u4e60/hadoop\u5927\u6570\u636e\u76f8\u5173\u7ecf\u9a8c\u8005\u4f18\u5148\u30024.\u6536\u96c6\u7814\u7a76\u884c\u4e1a\u5185\u65b0\u7684\u6280\u672f\u3001\u7b97\u6cd5\uff0c\u4e0d\u65ad\u63d0\u9ad8\u7cfb\u7edf\u5bf9\u56fe\u50cf\u8bc6\u522b\u7684\u6548\u7387\u4e0e\u51c6\u786e\u73875.\u7cbe\u901a\u56fe\u50cf\u589e\u5f3a\u3001\u76ee\u6807\u5b9a\u4f4d\u548c\u5206\u5272\u3001\u8fb9\u7f18\u63d0\u53d6\u7b49\u57fa\u672c\u56fe\u50cf\u5904\u7406\u7b97\u6cd5\u5efa\u5de5\u8def13,15\u53f7\u5929\u4fe1\u697c310\u5e7f\u5dde\u94f6\u8baf\u4fe1\u606f\u79d1\u6280\u6709\u9650\u516c\u53f8\u6c11\u8425\u516c\u53f850-150\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u8f6f\u4ef6\u5de5\u7a0b\u5e08", "\u516c\u53f8\u5730\u5740": "\u5efa\u5de5\u8def13,15\u53f7\u5929\u4fe1\u697c310"}, {"\u804c\u4f4d": "Python\u722c\u866b\u5de5\u7a0b\u5e08", "\u516c\u53f8": "\u5e7f\u4e1c\u5168\u5f69\u79d1\u6280\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "0.7-1.2\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u9910\u996e\u8865\u8d34\u7ee9\u6548\u5956\u91d1\u5e74\u7ec8\u5956\u91d1\u5458\u5de5\u65c5\u6e38\u5b9a\u671f\u4f53\u68c0\u4ea4\u901a\u8865\u8d34\u5468\u672b\u53cc\u4f11\u5730\u94c1\u5468\u8fb9\u516d\u9669\u4e00\u91d1", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde-\u5929\u6cb3\u533a|3-4\u5e74\u7ecf\u9a8c|\u5927\u4e13|\u62db1\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "1.\u8d1f\u8d23\u6570\u636e\u4e2d\u5fc3\u7684\u6570\u636e\u91c7\u96c6\u4e0e\u722c\u53d6\u3001\u89e3\u6790\u5904\u7406\u3001\u5165\u5e93\u53ca\u5907\u4efd\u7b49\u6570\u636e\u65e5\u5e38\u5de5\u4f5c\uff1b2.\u8d1f\u8d23\u6570\u636e\u7684\u6e05\u6d17\u3001\u6574\u7406\u3001\u6574\u5408\u53ca\u5408\u5e76\u7b49\u5de5\u4f5c\uff1b4.\u53c2\u4e0e\u9700\u6c42\u5206\u6790\u53ca\u8bbe\u8ba1\u6587\u6863\u3001\u4ee3\u7801\u7684\u7f16\u5199\uff1b5.\u6839\u636e\u5de5\u4f5c\u5b89\u6392\uff0c\u80fd\u6309\u65f6\u6309\u8d28\u5b8c\u6210\u9700\u6c42\u3002[\u804c\u4f4d\u8981\u6c42]1\u3001\u8ba1\u7b97\u673a\u79d1\u5b66\u4e0e\u6280\u672f\u3001\u8f6f\u4ef6\u5de5\u7a0b\u7b49\u76f8\u5173\u4e13\u4e1a\uff1b2\u3001\u719f\u6089linux\u5e73\u53f0\uff0c\u6709\u4e00\u5e74\u4ee5\u4e0aPython\u7f16\u7a0b\u7ecf\u9a8c\uff0c\u719f\u6089\u524d\u7aef\u5f00\u53d1\u7684\u76f8\u5173\u9886\u57df\u77e5\u8bc6\uff1b3\u3001\u719f\u6089Scrapy,XPath\u3001\u6b63\u5219\u8868\u8fbe\u5f0f\u539f\u7406\u7b49\uff1b4\u3001\u5177\u6709\u4f18\u79c0\u7684\u56e2\u961f\u5408\u4f5c\u548c\u6c9f\u901a\u534f\u4f5c\u80fd\u529b\uff0c\u5584\u4e8e\u5b66\u4e60\uff0c\u4e50\u4e8e\u5206\u4eab\uff0c\u80fd\u627f\u53d7\u8f83\u5927\u5de5\u4f5c\u538b\u529b\uff1b5\u3001\u7406\u89e3Web\u7b49\u6570\u636e\u6293\u53d6\u7684\u5de5\u4f5c\u539f\u7406\u53ca\u6d41\u7a0b\uff0c\u6709\u5b9e\u9645\u6570\u636e\u6293\u53d6\u7ecf\u9a8c\uff1b6\u3001\u719f\u7ec3\u4f7f\u7528Mysql/MongoDB/Redis\u7b49\u6570\u636e\u5e93\u6216\u961f\u5217\u3002[\u4f18\u5148\u6761\u4ef6]1.\u8bbe\u8ba1\u8fc7\u9664Scrapy\u6846\u67b6\u4ee5\u5916\u7684\u722c\u866b\u67b6\u6784\uff1b1.\u5bf9\u53cd\u722c\u866b\u6280\u672f\u6709\u4e00\u5b9a\u7684\u7406\u89e3\uff0c\u77e5\u9053\u5982\u4f55\u89e3\u51b3\u95ee\u9898\uff1b2.\u6709\u591a\u7ebf\u7a0b/\u591a\u8fdb\u7a0b\u76f8\u5173\u7684\u5f00\u53d1\u7ecf\u9a8c\uff1b3.\u6709\u9664\u4e86\u5199\u4ee3\u7801\u4e4b\u5916\u7684\u5176\u4ed6\u5174\u8da3\u7231\u597d\uff1b4.\u53d1\u5e03\u8fc7\u5f00\u6e90\u5e93\uff0c\u6216\u8005\u8d21\u732e\u8fc7\u5f00\u6e90\u4ee3\u7801\uff1b5.\u826f\u597d\u7684\u82f1\u6587\u9605\u8bfb\u80fd\u529b\u3002\u73e0\u6c5f\u65b0\u57ce\u4fdd\u5229\u514b\u6d1b\u7ef4\u5927\u53a6\u4e00\u671f17\u697c1710\u5ba4\u5e7f\u4e1c\u5168\u5f69\u79d1\u6280\u6709\u9650\u516c\u53f8\u6c11\u8425\u516c\u53f850-150\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u5927\u6570\u636e\u5f00\u53d1/\u5206\u6790\u6570\u636e\u5e93\u5de5\u7a0b\u5e08/\u7ba1\u7406\u5458\u722c\u866bPython\u6570\u636e\u6293\u53d6\u91c7\u96c6\u6e05\u6d17", "\u516c\u53f8\u5730\u5740": "\u73e0\u6c5f\u65b0\u57ce\u4fdd\u5229\u514b\u6d1b\u7ef4\u5927\u53a6\u4e00\u671f17\u697c1710\u5ba4"}, {"\u804c\u4f4d": "\u722c\u866b\u5f00\u53d1\u5de5\u7a0b\u5e08", "\u516c\u53f8": "\u5e7f\u5dde\u95ea\u8058\u7f51\u7edc\u79d1\u6280\u80a1\u4efd\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "1-1.5\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u5e26\u85aa\u5e74\u5047\u8282\u65e5\u798f\u5229\u4e94\u9669\u4e00\u91d1\u4e13\u4e1a\u57f9\u8bad\u5f39\u6027\u5de5\u4f5c\u7ee9\u6548\u5956\u91d1", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde-\u5929\u6cb3\u533a|1\u5e74\u7ecf\u9a8c|\u672c\u79d1|\u62db1\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "\u722c\u866b\u5de5\u7a0b\u5e08\u5de5\u4f5c\u5185\u5bb9\uff1a1\u3001\u8d1f\u8d23\u8bbe\u8ba1\u548c\u5f00\u53d1\u5206\u5e03\u5f0f\u7f51\u7edc\u722c\u866b\u7cfb\u7edf\uff1b2\u3001\u89e3\u51b3\u6280\u672f\u7591\u96be\u95ee\u9898\uff0c\u5305\u62ec\u53cd\u722c\u3001\u538b\u529b\u63a7\u5236\uff0c\u63d0\u5347\u7f51\u9875\u6293\u53d6\u7684\u6548\u7387\u548c\u8d28\u91cf\uff1b3\u3001\u6301\u7eed\u4f18\u5316\u7cfb\u7edf\uff0c\u63d0\u9ad8\u7cfb\u7edf\u7684\u81ea\u52a8\u5316\u3001\u7a33\u5b9a\u6027\u53ca\u6027\u80fd\u5c97\u4f4d\u8981\u6c42\uff1a1\u3001\u8ba1\u7b97\u673a\u53ca\u76f8\u5173\u4e13\u4e1a\uff0c\u672c\u79d1\u4ee5\u4e0a\u5b66\u5386\uff0c1\u5e74\u4ee5\u4e0a\u722c\u866b\u5f00\u53d1\u7ecf\u9a8c\uff1b2\u3001\u624e\u5b9e\u7684java\u57fa\u672c\u529f\uff0c\u4e86\u89e3JDK\u548c\u865a\u62df\u673a\uff1b\u719f\u6089\u5e38\u7528\u6570\u636e\u7ed3\u6784\u53ca\u7b97\u6cd53\u3001\u719f\u7ec3\u638c\u63e1Spring\u7b49\u6846\u67b6\uff0c\u6df1\u5165\u7406\u89e3\u6846\u67b6\u539f\u7406\u548c\u5b9e\u73b0\u4f18\u5148\uff1b4\u3001\u719f\u6089Linux\u7b49\u64cd\u4f5c\u7cfb\u7edf\uff0c\u4f1a\u5199shell,\u4e86\u89e3\u5e38\u89c1\u547d\u4ee4\uff1b5\u3001\u719f\u6089MYSQL\u6570\u636e\u5e93\uff0c\u4e86\u89e3sql\u4f18\u5316\u6280\u5de7\uff0c\u6709\u7ebf\u4e0a\u8c03\u4f18\u7ecf\u9a8c\u4f18\u5148\uff1b\u5e7f\u5dde\u5929\u6cb3\u533a\u4e2d\u5c71\u5927\u9053\u897f1138\u53f7\u5408\u751f\u9a8f\u666f\u5e7f\u573a1138\u53f71625\u5e7f\u5dde\u95ea\u8058\u7f51\u7edc\u79d1\u6280\u80a1\u4efd\u6709\u9650\u516c\u53f8\u6c11\u8425\u516c\u53f8\u5c11\u4e8e50\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u4e92\u8054\u7f51\u8f6f\u4ef6\u5f00\u53d1\u5de5\u7a0b\u5e08\u5927\u6570\u636e\u5f00\u53d1/\u5206\u6790\u722c\u866bjava\u722c\u866b\u5de5\u7a0b\u5e08java\u722c\u866b", "\u516c\u53f8\u5730\u5740": "\u5e7f\u5dde\u5929\u6cb3\u533a\u4e2d\u5c71\u5927\u9053\u897f1138\u53f7\u5408\u751f\u9a8f\u666f\u5e7f\u573a1138\u53f71625"}, {"\u804c\u4f4d": "\u722c\u866b\u5f00\u53d1\u5de5\u7a0b\u5e08", "\u516c\u53f8": "\u5e7f\u5dde\u901a\u6613\u79d1\u6280\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "0.8-1\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u4e94\u9669\u4e00\u91d1\u5458\u5de5\u65c5\u6e38\u4ea4\u901a\u8865\u8d34\u9910\u996e\u8865\u8d34\u5f39\u6027\u5de5\u4f5c\u5e74\u7ec8\u5956\u91d1\u7ee9\u6548\u5956\u91d1\u4e13\u4e1a\u57f9\u8bad\u5b9a\u671f\u4f53\u68c0", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde|2\u5e74\u7ecf\u9a8c|\u672c\u79d1|\u62db1\u4eba|03-14\u53d1\u5e03|\u666e\u901a\u8bdd", "\u804c\u4f4d\u4fe1\u606f": "1\u3001\u57fa\u4e8e\u4e1a\u52a1\u9879\u76ee\u6570\u636e\u6293\u53d6\u9700\u6c42\uff0c\u5b9a\u4e49\u548c\u5f00\u53d1\u6570\u636e\u6e90\uff1b2\u3001\u8d1f\u8d23\u5206\u5e03\u5f0f\u7f51\u7edc\u722c\u866b\u7cfb\u7edf\u7684\u67b6\u6784\u8bbe\u8ba1\u4e0e\u5f00\u53d1\uff08\u5982\u6293\u53d6\u8c03\u5ea6\uff0c\u591a\u6837\u5316\u6293\u53d6\uff0c\u9875\u9762\u89e3\u6790\u548c\u7ed3\u6784\u5316\u62bd\u53d6\uff0c\u6d77\u91cf\u6570\u636e\u5b58\u50a8\u548c\u8bfb\u53d6\u7b49\uff09\uff1b3\u3001\u8bbe\u8ba1\u548c\u4f18\u5316\u722c\u866b\u7b56\u7565\uff0c\u63d0\u5347\u722c\u866b\u6293\u53d6\u6548\u7387\u4e0e\u7a33\u5b9a\u6027\uff1b4\u3001\u7cfb\u7edf\u89e3\u51b3\u52a8\u6001\u7f51\u9875\u5185\u5bb9\u6293\u53d6\u3001\u6df1\u5ea6\u7f51\u9875\u5185\u5bb9\u6293\u53d6\u7684\u5173\u952e\u6280\u672f\uff1b5\u3001\u9488\u5bf9APP\u6570\u636e\u8fdb\u884c\u6293\u5305\u5206\u6790\uff0c\u722c\u53d6APP\u6570\u636e\u5e76\u6574\u5408\u5b58\u50a8\u3002\u4efb\u804c\u8981\u6c42\uff1a1\u3001\u672c\u79d1\u53ca\u4ee5\u4e0a\u5b66\u5386\uff0c\u4e24\u5e74\u4ee5\u4e0a\u722c\u866b\u9879\u76ee\u5f00\u53d1\u7ecf\u9a8c\uff0c\u719f\u6089\u5982scrapy\u7b49\u5f00\u6e90\u722c\u866b\u6846\u67b6\uff1b2\u3001\u719f\u6089\u722c\u866b\u6280\u672f,\u5305\u62ec\u4f46\u4e0d\u9650\u4e8e\u6b63\u5219\u8868\u8fbe\u5f0f\u3001\u8131\u58f3\u6280\u672f\u3001\u52a0\u5bc6\u3001\u4e8c\u7ef4\u7801\u8bc6\u522b\u3001\u6ed1\u5757\u9a8c\u8bc1\u7801\u7834\u89e3\u3001\u5206\u5e03\u5f0f\u722c\u866b\u8c03\u5ea6\u3001\u4ee3\u7406ip\u8c03\u5ea6\u7b97\u6cd5\u3001\u914d\u7f6e\u5316\u722c\u866b\u3001\u6388\u6743\u722c\u866b;3\u3001\u719f\u6089MySql\u3001Oracle\u3001MongoDB\u4e00\u79cd\u6216\u591a\u79cd\u6570\u636e\u5e93\u5f00\u53d1\u6280\u672f\uff0c\u6709\u8f83\u597d\u7684\u6570\u636e\u5e93\u8bbe\u8ba1\u80fd\u529b\uff1b4\u3001\u719f\u6089\u7f51\u9875\u6293\u53d6\u539f\u7406\u53ca\u6280\u672f\u3001\u6df1\u5ea6\u6293\u53d6\u3001\u52a8\u6001\u7f51\u9875\u6280\u672f\u6293\u53d6\u3001\u6d4f\u89c8\u5668\u6a21\u62df\u6293\u53d6\u6280\u672f\uff0c\u4ece\u7ed3\u6784\u5316\u7684\u548c\u975e\u7ed3\u6784\u5316\u7684\u6570\u636e\u4e2d\u83b7\u53d6\u4fe1\u606f\uff1b5\u3001\u53c2\u4e0e\u8fc7\u722c\u866b\u7cfb\u7edf\u7684\u67b6\u6784\u8bbe\u8ba1\u4e0e\u5f00\u53d1\uff0c\u5e76\u5bf9\u5e38\u89c1\u7684\u53cd\u722c\u866b\u7b56\u7565\u6709\u4e00\u5b9a\u7684\u638c\u63e1\uff1b6\u3001\u5177\u6709\u8f83\u5f3a\u7684\u5206\u6790\u548c\u89e3\u51b3\u95ee\u9898\u80fd\u529b\uff0c\u826f\u597d\u7684\u6c9f\u901a\u80fd\u529b\u548c\u56e2\u961f\u5408\u4f5c\u7cbe\u795e\uff0c\u4e50\u4e8e\u63a5\u53d7\u6311\u6218\u3002\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u7ff0\u666f\u8def1\u53f7\u91d1\u661f\u5927\u53a614\u697c\uff08\u4e94\u5c71\u8def\u91d1\u5c71\u5927\u53a6\u65c1\u8fb9\uff0c\u534e\u5357\u5e08\u8303\u5927\u5b66\u9644\u8fd1\uff09\u5e7f\u5dde\u901a\u6613\u79d1\u6280\u6709\u9650\u516c\u53f8\u6c11\u8425\u516c\u53f850-150\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u9ad8\u7ea7\u8f6f\u4ef6\u5de5\u7a0b\u5e08\u8f6f\u4ef6\u5de5\u7a0b\u5e08\u722c\u866bPython\u6570\u636e\u5e93\u6570\u636e\u6293\u53d6", "\u516c\u53f8\u5730\u5740": "\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u7ff0\u666f\u8def1\u53f7\u91d1\u661f\u5927\u53a614\u697c\uff08\u4e94\u5c71\u8def\u91d1\u5c71\u5927\u53a6\u65c1\u8fb9\uff0c\u534e\u5357\u5e08\u8303\u5927\u5b66\u9644\u8fd1\uff09"}, {"\u804c\u4f4d": "Java\u5de5\u7a0b\u5e08\uff08\u722c\u866b\u65b9\u5411\uff09", "\u516c\u53f8": "\u4e1c\u839e\u4f18\u8d38\u7f51\u7edc\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "0.8-1.3\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u4e94\u9669\u4e00\u91d1\u5458\u5de5\u65c5\u6e38\u4e13\u4e1a\u57f9\u8bad\u7ee9\u6548\u5956\u91d1\u8282\u65e5\u798f\u5229\u80a1\u7968\u671f\u6743", "\u62db\u8058\u4fe1\u606f": "\u4e1c\u839e-\u5357\u57ce\u533a|3-4\u5e74\u7ecf\u9a8c|\u5927\u4e13|\u62db2\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "1\u3001\u5206\u5e03\u5f0f\u7f51\u7edc\u722c\u866b\u7cfb\u7edf\u7684\u8bbe\u8ba1\u53ca\u5f00\u53d12\u3001\u4e92\u8054\u7f51\u7ad9\u3001\u7f51\u9875\u3001\u94fe\u63a5\u7684\u7279\u5f81\u6316\u6398\uff0c\u8fdb\u884c\u7f51\u9875\u6293\u53d6\u3001\u6570\u636e\u63d0\u53d6\u3001\u6e05\u6d17\u3001\u5165\u5e93\uff1b3\u3001\u8d1f\u8d23\u722c\u866b\u6838\u5fc3\u7b97\u6cd5\u7684\u7b56\u7565\u4f18\u5316\u7814\u7a76\uff0c\u5145\u5206\u5229\u7528\u8d44\u6e90\uff0c\u907f\u514d\u9650\u5236\uff1b4\u3001\u4f18\u5316\u641c\u7d22\u3001\u5339\u914d\u3001\u6293\u53d6\u7b49\u5173\u952e\u7a0b\u5e8f\u7684\u6027\u80fd\u53ca\u6548\u7387\u3002\u4efb\u804c\u8981\u6c42\uff1a1\u30012\u5e74\u4ee5\u4e0ajava\u7ecf\u9a8c,\u6709\u722c\u866b\u5f00\u53d1\u7ecf\u9a8c,\u719f\u7ec3\u5e94\u7528\u591a\u7ebf\u7a0b\u6280\u672f.2\u3001\u719f\u7ec3\u4f7f\u7528\u6b63\u5219\u8868\u8fbe\u5f0f\u3001csspath\u3001xpath\u7b49\uff0c\u80fd\u591f\u4ece\u7ed3\u6784\u5316\u7684\u548c\u975e\u7ed3\u6784\u5316\u7684\u6570\u636e\u4e2d\u83b7\u53d6\u4fe1\u606f\uff1b3\u3001\u719f\u7ec3\u4f7f\u7528Mysql\u6570\u636e\u5e934\u3001\u719f\u7ec3\u4e00\u79cd\u5f00\u6e90\u722c\u866b\u6846\u67b6\uff0c\u5982scrapy\u3001webmagic\u3001nutch\u3001heritrix\u7b49\uff0c\u6709\u5f00\u53d1\u722c\u866b\u6846\u67b6\u7ecf\u9a8c\u4f18\u5148\uff1b\u719f\u6089\u53cd\u722c\u866b\u3001\u9a8c\u8bc1\u7801\u8bc6\u522b\u6280\u672f\u8005\u4f18\u5148\uff1b5\u3001\u5177\u6709\u6570\u636e\u6316\u6398\u3001\u81ea\u7136\u8bed\u8a00\u5904\u7406\u3001\u4fe1\u606f\u68c0\u7d22\u3001\u673a\u5668\u5b66\u4e60\u80cc\u666f\u8005\u4f18\u5148\uff1b\u516c\u53f8\u4ecb\u7ecd\uff1a\u4e1c\u839e\u4f18\u8d38\u7f51\u7edc\u6709\u9650\u516c\u53f8\uff0c\u5176\u65d7\u4e0b\u7684\u6838\u5fc3\u5e73\u53f0\u3010\u4f18\u8d38\u7f51\u3011www.B2B3.com\uff0c\u4e13\u6ce8\u4e8e\u4e3a\u5168\u7403\u8d38\u6613\u4ece\u4e1a\u8005\u63d0\u4f9b\u7cbe\u51c6\u7684\u5927\u6570\u636e\u8425\u9500\u670d\u52a1\uff0c\u52a9\u529b\u5168\u7403\u4f9b\u5e94\u5546\u9500\u552e\u4e1a\u7ee9\u7684\u589e\u957f\uff0c\u5e2e\u52a9\u5168\u7403\u8fdb\u53e3\u5546\u53d1\u73b0***\u4f9b\u5e94\u5546\uff0c\u4fc3\u8fdb\u5168\u7403\u8d38\u6613\u66f4\u5b89\u5168\u66f4\u9ad8\u6548\uff1b\u4f18\u8d38\u7f51\u62e5\u6709\u591a\u79cd\u4e1a\u52a1\u6a21\u5f0f\uff0c\u4e3b\u8981\u6536\u5165\u6765\u81ea\u4e8e\u4ed8\u8d39\u5e10\u53f7\u3001\u5e7f\u544a\u9500\u552e\u53ca\u591a\u79cd\u5b9a\u5236\u6570\u636e\u89e3\u51b3\u65b9\u6848\uff01\u6709\u610f\u8005\u53ef\u76f4\u63a5\u6765\u7535\u4eba\u4e8b\u90e8\u54a8\u8be2\u3001\u6210\u5c0f\u59d0\uff1a0769-22312980\u7f8e\u4e30\u8def\u521b\u666f\u697c3F\u5916\u4ea4\u5708\u4e1c\u839e\u4f18\u8d38\u7f51\u7edc\u6709\u9650\u516c\u53f8\u5408\u8d44\u5c11\u4e8e50\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u4e92\u8054\u7f51\u8f6f\u4ef6\u5f00\u53d1\u5de5\u7a0b\u5e08\u5927\u6570\u636e\u5f00\u53d1/\u5206\u6790Java\u5de5\u7a0b\u5e08\u722c\u866b\u5f00\u53d1\u4e92\u8054\u7f51\u5f00\u53d1\u6570\u636e\u6316\u6398\u5f00\u53d1\u7a0b\u5e8f\u5458\u7f51\u7ad9\u5de5\u7a0b\u5e08", "\u516c\u53f8\u5730\u5740": "\u7f8e\u4e30\u8def\u521b\u666f\u697c3F\u5916\u4ea4\u5708"}, {"\u804c\u4f4d": "\u9ad8\u7ea7\u722c\u866b\u5de5\u7a0b\u5e08", "\u516c\u53f8": "\u5e7f\u5dde\u6570\u8bf4\u6545\u4e8b\u4fe1\u606f\u79d1\u6280\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "1.5-2\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u4e94\u9669\u4e00\u91d1\u5f39\u6027\u5de5\u4f5c\u5b9a\u671f\u4f53\u68c0\u5e74\u7ec8\u5956\u91d1\u5458\u5de5\u65c5\u6e38", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde-\u5929\u6cb3\u533a|3-4\u5e74\u7ecf\u9a8c|\u672c\u79d1|\u62db1\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "\u5c97\u4f4d\u804c\u8d231\u3001\u8d1f\u8d23\u5404\u7c7b\u7535\u5546\u5e73\u53f0\u548capp\u7aef\u6570\u636e\u6293\u53d6\u548c\u5e73\u53f0\u642d\u5efa\uff1b2\u3001\u8d1f\u8d23\u5bf9\u722c\u53d6\u6570\u636e\u8fdb\u884c\u5206\u7c7b\u548c\u89e3\u6790\uff1b3\u3001\u8d1f\u8d23\u5f00\u53d1\u9ad8\u6027\u80fd\u6293\u53d6\u67b6\u6784\uff0c\u652f\u6301\u4e1a\u52a1\u53d1\u5c55\uff1b4\u3001\u8d1f\u8d23\u722c\u866b\u6280\u672f\u516c\u5173\u548c\u5e73\u53f0\u8fd0\u7ef4\u76f8\u5173\u5de5\u4f5c\uff1b\u4efb\u804c\u8981\u6c421\u3001\u672c\u79d1\u53ca\u4ee5\u4e0a\u5b66\u5386\uff0c\u4e09\u5e74\u53ca\u4ee5\u4e0a\u76f8\u5173\u5de5\u4f5c\u7ecf\u9a8c\uff1b2\u3001\u719f\u6089linux\u5e73\u53f0\uff0c\u719f\u7ec3\u638c\u63e1java/shell/http\u534f\u8bae\uff0c\u719f\u6089HTML\u3001DOM\u3001XPath\uff0c\u638c\u63e1git\u3001maven\u3001svn\u7b49\u5de5\u5177\u548c\u5b9e\u8df5\uff0c\u6ce8\u91cd\u5de5\u7a0b\u89c4\u8303\uff1b3\u3001\u719f\u6089app\u7aef\u6570\u636e\u6293\u53d6\uff08\u6709\u9006\u5411\u3001\u8131\u58f3\u7b49\u7ecf\u9a8c\uff09\uff0c\u638c\u63e1Apktool\u3001dex2jar\u3001JD-GUI\u7b49\u5de5\u5177\uff0c\u6293\u53d6\u8fc7\u4e3b\u6d41\u7535\u5546\u5e73\u53f0\u548c\u4e3b\u6d41app\u8f6f\u4ef6\uff1b4\u3001\u81f3\u5c111\u5e74\u7684\u5206\u5e03\u5f0f\u722c\u866b\u5f00\u53d1\u7ecf\u9a8c\uff0c\u719f\u6089\u6d4f\u89c8\u5668\u5185\u6838\uff0c\u6709cef\u3001webkit\u5f00\u53d1\u7ecf\u9a8c\u4f18\u5148\uff1b5\u3001\u6709\u5b89\u5353/iOS\u76f8\u5173\u5f00\u53d1\u7ecf\u9a8c\u8005\u4f18\u5148\uff1b6\u3001\u6027\u683c\u5f00\u6717\u3001\u5584\u4e8e\u6c9f\u901a\uff0c\u6709\u826f\u597d\u7684\u81ea\u6211\u9a71\u52a8\u5b66\u4e60\u80fd\u529b\uff0c\u6ce8\u91cd\u6548\u7387\u548c\u56e2\u961f\u610f\u8bc6\uff0c\u6709\u56e2\u961f\u7ba1\u7406\u7ecf\u9a8c\u4f18\u5148\uff1b\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u73e0\u6c5f\u65b0\u57ce\u9ec4\u57d4\u5927\u9053\u897f100\u53f7\u5bcc\u529b\u76c8\u6cf0A\u5854701\u5e7f\u5dde\u6570\u8bf4\u6545\u4e8b\u4fe1\u606f\u79d1\u6280\u6709\u9650\u516c\u53f8\u6c11\u8425\u516c\u53f8150-500\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u9ad8\u7ea7\u8f6f\u4ef6\u5de5\u7a0b\u5e08\u6570\u636e\u5e93\u5de5\u7a0b\u5e08/\u7ba1\u7406\u5458java", "\u516c\u53f8\u5730\u5740": "\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u73e0\u6c5f\u65b0\u57ce\u9ec4\u57d4\u5927\u9053\u897f100\u53f7\u5bcc\u529b\u76c8\u6cf0A\u5854701"}, {"\u804c\u4f4d": "\u722c\u866b\u5de5\u7a0b\u5e08", "\u516c\u53f8": "\u5e7f\u5dde\u4e2d\u5730\u4f1a\u8d44\u8baf\u53d1\u5c55\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "10-20\u4e07/\u5e74", "\u516c\u53f8\u798f\u5229": "\u4e94\u9669\u4e00\u91d1\u5e74\u7ec8\u5956\u91d1\u8865\u5145\u533b\u7597\u4fdd\u9669\u8865\u5145\u516c\u79ef\u91d1\u7ee9\u6548\u5956\u91d1", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde|3-4\u5e74\u7ecf\u9a8c|\u672c\u79d1|\u62db2\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "\u5c97\u4f4d\u804c\u8d23\uff1a1.\u8d1f\u8d23\u5206\u5e03\u5f0f\u7f51\u7edc\u722c\u866b\u7cfb\u7edf\u7684\u67b6\u6784\u8bbe\u8ba1\u4e0e\u5f00\u53d1\uff1b2.\u4e92\u8054\u7f51\u7ad9\u3001\u7f51\u9875\u3001\u94fe\u63a5\u7684\u7279\u5f81\u6316\u6398\uff0c\u6839\u636e\u4e1a\u52a1\u8981\u6c42\u8d1f\u8d23\u6293\u53d6\u4e92\u8054\u7f51\u5404\u79cd\u5f62\u5f0f\u6570\u636e\uff0c\u5982\u7f51\u9875\u3001pdf\u3001word\u7b49\u6570\u636e\u6293\u53d6\u3001\u6570\u636e\u63d0\u53d6\u3001\u6e05\u6d17\u3001\u5165\u5e93\uff1b3.\u8d1f\u8d23\u722c\u866b\u6838\u5fc3\u7b97\u6cd5\u7684\u7b56\u7565\u4f18\u5316\u7814\u7a76\uff0c\u5145\u5206\u5229\u7528\u8d44\u6e90\uff0c\u63d0\u5347\u7f51\u9875\u6293\u53d6\u7684\u6548\u7387\u548c\u8d28\u91cf\uff1b4.\u4f18\u5316\u641c\u7d22\u3001\u5339\u914d\u3001\u6293\u53d6\u7b49\u5173\u952e\u7a0b\u5e8f\u7684\u6027\u80fd\u53ca\u6548\u7387\uff1b5.\u53d6\u4e1a\u52a1\u9700\u6c42\u76f8\u5173\u7f51\u9875\u3001pdf\u7b49\u5f62\u5f0f\u6570\u636e\uff0c\u80fd\u591f\u603b\u7ed3\u5206\u6790\u4e0d\u540c\u7f51\u7ad9\u7684\u7ed3\u6784\u7279\u70b9\u4e0e\u89c4\u5f8b\uff1b6\u3001\u8d1f\u8d23\u722c\u866b\u4f53\u7cfb\u548c\u5e73\u53f0\u7ba1\u7406\u53ca\u4efb\u52a1\u5b89\u6392\uff0c\u5305\u62ec\u4efb\u52a1\u7ba1\u7406\u3001\u4ee3\u7801\u7ba1\u7406\u3001\u4ee3\u7801\u5ba1\u6838\u3001\u722c\u866b\u8fd0\u884c\u76d1\u63a7\u53ca\u7ba1\u7406\u3001\u95ee\u9898\u53cd\u9988\u53ca\u89e3\u51b3\u7b49\uff1b6.\u8fdb\u884c\u6280\u672f\u5206\u4eab\u4e0e\u4eba\u5458\u57f9\u8bad\u3002\u4efb\u804c\u8981\u6c42\uff1a1.\u672c\u79d1\u4ee5\u4e0a\u5b66\u5386\uff0c2\u5e74\u4ee5\u4e0a\u722c\u866b\u5f00\u53d1\u76f8\u5173\u5de5\u4f5c\u7ecf\u9a8c\uff0c\u6709\u5927\u89c4\u6a21\u7f51\u7edc\u722c\u866b\u5f00\u53d1\u7ecf\u9a8c\u4f18\u5148\uff1b2.\u7cbe\u901aPython\uff1b3.\u719f\u6089\u591a\u7ebf\u7a0b\u3001\u7f51\u7edc\u7f16\u7a0b\u3001\u4e91\u670d\u52a1\uff0c\u7cbe\u901a\u7f51\u9875\u6293\u53d6\u539f\u7406\u53ca\u6280\u672f\uff0c\u7cbe\u901a\u6b63\u5219\u8868\u8fbe\u5f0f\uff0c\u80fd\u4ece\u7ed3\u6784\u5316\u7684\u548c\u975e\u7ed3\u6784\u5316\u7684\u6570\u636e\u4e2d\u83b7\u53d6\u4fe1\u606f\uff1b4.\u719f\u6089\u722c\u866b\u3001\u79cd\u5b50\u3001\u53bb\u91cd\u3001\u63d0\u53d6\u3001\u8fc7\u6ee4\u3001\u5206\u53d1\u3001DNScache\u3001\u5f02\u6b65\u5904\u7406\u7b49\u6982\u5ff5\u548c\u8fc7\u7a0b\uff1b5.\u5177\u5907\u4fe1\u606f\u68c0\u7d22\u3001web\u6316\u6398\u7b49\u641c\u7d22\u5f15\u64ce\u76f8\u5173\u77e5\u8bc6\uff0c\u6709\u4ece\u4e8b\u7f51\u7edc\u722c\u866b\u3001\u7f51\u9875\u53bb\u91cd\u3001\u7f51\u9875\u4fe1\u606f\u6293\u53d6\u3001\u7f51\u9875\u5206\u7c7b\u4e2d\u81f3\u5c11\u4e00\u79cd\u7a0b\u5e8f\u5f00\u53d1\u7ecf\u9a8c\uff1b6.\u719f\u7ec3\u4f7f\u7528\u6570\u636e\u5e93\uff0c\u81f3\u5c11\u7cbe\u901a\u4e00\u79cd\u5173\u7cfb\u578b\u6570\u636e\u5e93\u7684\u5f00\u53d1\uff0c\u5904\u7406\u8fc7\u5343\u4e07\u7ea7\u522b\u4ee5\u4e0a\u6570\u636e\uff0c\u638c\u63e1Redis\u3001MongoDB\u3001Hive\u7b49\u5e38\u7528NoSQL\u6280\u672f\u5e76\u5177\u6709\u5b9e\u6218\u7ecf\u9a8c\uff0c\u719f\u6089hadoop\u8005\u4f18\u5148\uff1b7.\u7cbe\u901a\u81f3\u5c11\u4e00\u79cd\u5f00\u6e90\u722c\u866b\u6846\u67b6\uff0c\u5982Scrapy\u3001Webmagic\u3001Nutch\u3001Heritrix\u7b49\uff0c\u6709\u5f00\u53d1\u722c\u866b\u6846\u67b6\u7ecf\u9a8c\u4f18\u5148\uff1b8.\u719f\u6089\u9632\u91c7\u96c6\u3001\u9a8c\u8bc1\u7801\u8bc6\u522b\u3001\u56fe\u50cf\u8bc6\u522b\u3001\u5c01IP\u7b49\u53cd\u722c\u6280\u672f\uff1b9.\u5177\u6709\u6587\u672c\u5206\u6790\u3001\u6570\u636e\u6316\u6398\u3001\u81ea\u7136\u8bed\u8a00\u5904\u7406\u3001\u4fe1\u606f\u68c0\u7d22\u3001\u673a\u5668\u5b66\u4e60\u80cc\u666f\u8005\u4f18\u5148\uff1b10.\u7cbe\u901aJava\u6216C++\u8bed\u8a00\u4f18\u5148\u3002\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u73e0\u6c5f\u65b0\u57ce\u4e34\u6c5f\u5927\u905357\u53f7\u4e2d\u548c\u5e7f\u573a8\u697c\u5e7f\u5dde\u4e2d\u5730\u4f1a\u8d44\u8baf\u53d1\u5c55\u6709\u9650\u516c\u53f8\u975e\u8425\u5229\u7ec4\u7ec750-150\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u4fe1\u606f\u6280\u672f\u7ecf\u7406/\u4e3b\u7ba1\u722c\u866b", "\u516c\u53f8\u5730\u5740": "\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u73e0\u6c5f\u65b0\u57ce\u4e34\u6c5f\u5927\u905357\u53f7\u4e2d\u548c\u5e7f\u573a8\u697c"}, {"\u804c\u4f4d": "\u670d\u52a1\u7aef\u811a\u672c\u5de5\u7a0b\u5e08\uff08\u722c\u866b\uff09", "\u516c\u53f8": "\u5317\u4eac\u55e8\u9038\u9152\u5e97\u7ba1\u7406\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "1-1.5\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "\u51fa\u56fd\u673a\u4f1a\u5f39\u6027\u5de5\u4f5c\u5e26\u85aa\u5e74\u5047\u5e74\u5e95\u53cc\u85aa\u4e94\u9669\u4e00\u91d1\u5916\u6d3e\u6d25\u8d34\u53d1\u5c55\u7a7a\u95f4\u5927", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde-\u5929\u6cb3\u533a|1\u5e74\u7ecf\u9a8c|\u62db2\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "\u804c\u4f4d\u63cf\u8ff0\uff1a\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u5458\u6751\u56db\u6a2a\u8def128\u53f7\u7ea2\u4e13\u5382\u827a\u672f\u521b\u610f\u56ed\u533aC8\u680b2F\u5317\u4eac\u55e8\u9038\u9152\u5e97\u7ba1\u7406\u6709\u9650\u516c\u53f8\u6c11\u8425\u516c\u53f850-150\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u6280\u672f\u652f\u6301/\u7ef4\u62a4\u5de5\u7a0b\u5e08\u8ba1\u91cf\u5de5\u7a0b\u5e08\u722c\u866bIT\u670d\u52a1\u7aef\u811a\u672c", "\u516c\u53f8\u5730\u5740": "\u5e7f\u5dde\u5e02\u5929\u6cb3\u533a\u5458\u6751\u56db\u6a2a\u8def128\u53f7\u7ea2\u4e13\u5382\u827a\u672f\u521b\u610f\u56ed\u533aC8\u680b2F"}, {"\u804c\u4f4d": "SCRAPY\u6280\u672f\u987e\u95ee(\u722c\u866b)(\u804c\u4f4d\u7f16\u53f7\uff1a00410)", "\u516c\u53f8": "\u5e7f\u5dde\u8d5b\u610f\u4fe1\u606f\u79d1\u6280\u80a1\u4efd\u6709\u9650\u516c\u53f8", "\u85aa\u8d44": "1-1.5\u4e07/\u6708", "\u516c\u53f8\u798f\u5229": "", "\u62db\u8058\u4fe1\u606f": "\u5e7f\u5dde|2\u5e74\u7ecf\u9a8c|\u672c\u79d1|\u62db3\u4eba|03-14\u53d1\u5e03", "\u804c\u4f4d\u4fe1\u606f": "\u5c97\u4f4d\u804c\u8d23\uff1a1.\u8d1f\u8d23\u5927\u6570\u636e\u9879\u76ee\u6570\u636e\u96c6\u6210\u5de5\u4f5c\u7684\u5f00\u53d1\u3001\u6d4b\u8bd5\u4e0e\u8c03\u4f18\uff1b2.\u8d1f\u8d23\u9879\u76ee\u4e2d\u6570\u636e\u5904\u7406\u5de5\u4f5c\uff08\u6570\u636e\u91c7\u96c6\u3001\u6e05\u6d17\u3001\u6c47\u603b\u3001\u96c6\u6210\u7b49\uff09\u4efb\u804c\u8981\u6c42\uff1a1.\u672c\u79d1\u53ca\u4ee5\u4e0a\u5b66\u5386\uff0c\u8ba1\u7b97\u673a\u76f8\u5173\u4e13\u4e1a\uff0c\u719f\u6089\u6570\u636e\u5206\u6790\u7cfb\u7edf\u4f53\u7cfb\u67b6\u6784\uff0c\u719f\u6089\u6570\u636e\u4ed3\u5e93\u5efa\u6a21\u7406\u8bba\u3001\u6a21\u578b\u8bbe\u8ba1\u601d\u8def,\u5e76\u7528\u4e8e\u5b9e\u9645\u9879\u76ee\u5b9e\u8df5\uff1b2.\u719f\u7ec3\u638c\u63e1Python\u3001JAVA\u7f16\u7a0b\u8bed\u8a00\uff1b3.\u719f\u6089linux\u7cfb\u7edf\u73af\u5883\uff1b4.\u719f\u7ec3\u638c\u63e1SQL\u811a\u672c\u7f16\u5199\u4e0e\u8c03\u4f18\uff1b5.\u719f\u7ec3\u638c\u63e1hiveSQL\u5f00\u53d1\uff1b6.\u719f\u6089hdfs\u3001mapreduce\u3001spark\u5b9e\u73b0\u539f\u7406\u4e0e\u673a\u5236\uff1b7.\u719f\u7ec3\u638c\u63e1kafka\u3001storm\u3001sparkstreaming\u7f16\u7a0b\u6846\u67b6\uff0c\u5e76\u5177\u5907\u9879\u76ee\u5b9e\u65bd\u7ecf\u9a8c\uff1b(\u5929\u6cb3\u533a)\u5e7f\u5dde\u8d5b\u610f\u4fe1\u606f\u79d1\u6280\u80a1\u4efd\u6709\u9650\u516c\u53f8\u4e0a\u5e02\u516c\u53f81000-5000\u4eba", "\u804c\u80fd\u7c7b\u522b": "\u9ad8\u7ea7\u8f6f\u4ef6\u5de5\u7a0b\u5e08", "\u516c\u53f8\u5730\u5740": "(\u5929\u6cb3\u533a)"}]